<?php
//initialize Celsius value
$Celsius = 10;
//Convert Celsius to Fahrenheit
$Fahrenheit =  ($Celsius * 9/5) + 32 ;

echo ("The Celsius is: $Celsius and Fahrenheit is: $Celsius To Fahrenheit \n");

//initialize Fahrenheight value
$Fahrenheit = 35;
//Convert Fahrenheit to Celsius
$FahrenheitToCelsius =  ($Fahrenheit - 32) * 5/9;
echo ("The Fahrenheit is: $Fahrenheit and Celsius is: $Fahrenheit To Celsius");